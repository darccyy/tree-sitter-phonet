{
  "name": "phonet",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statement"
      }
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "command"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "STRING",
              "value": ";"
            }
          ]
        }
      ]
    },
    "command": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "class"
        }
      ]
    },
    "class": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "regex"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "regex": {
      "type": "PATTERN",
      "value": "[^;\\n]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

