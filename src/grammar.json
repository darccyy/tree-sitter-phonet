{
  "name": "phonet",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "statement"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "STRING",
              "value": ";"
            }
          ]
        }
      ]
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "mode"
        },
        {
          "type": "SYMBOL",
          "name": "class"
        },
        {
          "type": "SYMBOL",
          "name": "rule"
        },
        {
          "type": "SYMBOL",
          "name": "test"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[^;\\n]+"
        }
      ]
    },
    "mode": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "~"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "<"
                },
                {
                  "type": "PATTERN",
                  "value": "[^>;\\n]+"
                },
                {
                  "type": "STRING",
                  "value": ">"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "/"
                },
                {
                  "type": "PATTERN",
                  "value": "[^>;\\n]+"
                },
                {
                  "type": "STRING",
                  "value": "/"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "["
                },
                {
                  "type": "PATTERN",
                  "value": "[^>;\\n]+"
                },
                {
                  "type": "STRING",
                  "value": "]"
                }
              ]
            }
          ]
        }
      ]
    },
    "class": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "regex"
        }
      ]
    },
    "rule": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "positive"
            },
            {
              "type": "SYMBOL",
              "name": "negative"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "regex"
        }
      ]
    },
    "test": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "?"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "positive"
            },
            {
              "type": "SYMBOL",
              "name": "negative"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        }
      ]
    },
    "positive": {
      "type": "STRING",
      "value": "+"
    },
    "negative": {
      "type": "STRING",
      "value": "!"
    },
    "name": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "word": {
      "type": "PATTERN",
      "value": "[^;\\n]+"
    },
    "regex": {
      "type": "PATTERN",
      "value": "[^;\\n]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

